---
layout: base.hbs
---

<!DOCTYPE html>
<html class="no-js">
<head>
  {{> head }}
</head>
<body {{#if bodyClass}}class="{{bodyClass}}"{{/if}}>
  <header class="site-header" role="banner">
    <div class="container">
      {{> header }}
    </div>
  </header>
  <main class="site-main" role="main">
    <div class="container">
      {{> body }}
    </div>
  </main>
  <footer class="site-footer" role="contentinfo">
    <div class="container">
      {{> footer }}
    </div>
  </footer>
  {{> scripts }}
</body>
</html>
